<?xml version="1.0" encoding="UTF-8"?>
<Configuration monitorInterval="60">
    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} %msg%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <root level="INFO">
            <Appender-ref ref="SERVER_LOG"/>
            <Appender-ref ref="CONSOLE"/>
        </root>
    </Loggers>
</Configuration>
